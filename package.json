{
  "name": "menu-generator",
  "productName": "Menu Generator",
  "version": "1.0.0",
  "description": "Generates menus and shopping lists",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start-debug": "electron . --nolazy --inspect-brk=9229",
    "package-win": "electron-packager . menu-generator --extra-resource=./resources/Dict.json --extra-resource=./resources/settings.png --overwrite --ignore=\\.gitignore --ignore=./resources --ignore=./release-builds  --ignore=./PDFs --asar=true --platform=win32 --arch=ia32 --icon=./resources/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Menu Generator'",
    "package-mac": "electron-packager . menu-generator --mac --extra-resource=./resources/Dict.json --extra-resource=./resources/settings.png --overwrite --ignore=\\.gitignore --ignore=./resources --ignore=./release-builds  --ignore=./PDFs --asar=true --platform=darwin --arch=x64 --icon=./resources/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Menu Generator'"
  },
  "keywords": [],
  "author": "Lisa Karlin",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.3"
  },
  "dependencies": {
    "awesomplete": "^1.1.2",
    "choices.js": "^3.0.4",
    "electron-installer-windows": "^1.1.0",
    "electron-packager": "^11.0.0",
    "electron-pdf": "^1.2.2",
    "form-data": "^2.3.1",
    "fs": "0.0.1-security",
    "tags-input": "^1.1.1"
  }
}
